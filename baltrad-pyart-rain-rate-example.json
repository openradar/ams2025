:"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"display.plot_ppi(\"RATE\", 0, vmin=0, vmax=50.0)","key":"uZHiRBJxkU"},{"type":"output","id":"f2zSEVHPQtNaKiT4GoKaf","data":[],"key":"uvXNVWh5Wd"}],"key":"sdJYFHRFTD"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Create a new RaveIO object from the Py-ART radar object and write this out using Rave","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Rfl5Tn2ixv"}],"key":"wQheoLvhOb"}],"key":"NobAulgcqq"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"rio_out = bridge.radar2raveio(radar)","key":"JwsUvJGs7N"},{"type":"output","id":"ZRD48H0tWMQ9vLYBCckZ6","data":[],"key":"QETjb4xySL"}],"key":"MVGUQf86WK"},{"type":"block","kind":"notebook-code","data":{},"children":[{"type":"code","lang":"python","executable":true,"value":"container = _raveio.new()\ncontainer.object = rio_out.object\ncontainer.save(\"data/WKR_201307082030_with_rain_rate.h5\")\n\nimport os\n\nprint(\n    \"ODIM_H5 file is %i bytes large\"\n    % os.path.getsize(\"data/WKR_201307082030_with_rain_rate.h5\")\n)","key":"AB3zHlZJaU"},{"type":"output","id":"0uXlPS18F4aeEtwQ1le1G","data":[],"key":"wU9r8X5xFA"}],"key":"gZrUoB0SUb"}],"key":"ml8zixfUbl"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"Compositing","url":"/baltrad-compositing","group":"BALTRAD"},"next":{"title":"Doppler Velocity Dealiasing with Py-ART","url":"/pyart-baltrad-dealias-example","group":"BALTRAD"}}},"domain":"http://localhost:3000"}